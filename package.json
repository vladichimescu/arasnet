{
  "name": "arasnet",
  "version": "0.1.0",
  "workspaces": {
    "packages": [
      "portal",
      "server"
    ]
  },
  "private": true,
  "devDependencies": {
    "@trivago/prettier-plugin-sort-imports": "^4.3.0",
    "prettier": "^3.3.2"
  },
  "scripts": {
    "clean": "rm -rf .yarn portal/build && find . -name 'node_modules' -type d -prune -exec rm -rf '{}' +",
    "format": "prettier --write \"**/*.{js,jsx,json,md}\"",
    "start": "yarn workspaces foreach --all --parallel run start",
    "build": "yarn workspaces foreach --all --parallel run build",
    "serve": "yarn workspaces foreach --all --parallel run serve"
  },
  "engines": {
    "node": "20.15.0",
    "yarn": "4.3.1"
  },
  "volta": {
    "node": "20.15.0",
    "yarn": "4.3.1"
  }
}
