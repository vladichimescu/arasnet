{
  "name": "@arasnet/portal",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@ag-grid-community/core": "^32.1.0",
    "@ag-grid-community/infinite-row-model": "^32.1.0",
    "@ag-grid-community/react": "^32.1.0",
    "@ag-grid-community/styles": "^32.1.0",
    "@arasnet/functions": "*",
    "@arasnet/types": "*",
    "@fortawesome/fontawesome-free": "^6.6.0",
    "axios": "^1.7.4",
    "i18next": "^23.12.6",
    "i18next-icu": "^2.3.0",
    "intl-messageformat": "^10.5.14",
    "milligram": "^1.4.1",
    "nprogress": "^0.2.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-i18next": "^15.0.1",
    "react-phone-input-2": "^2.15.1",
    "react-router-dom": "^6.26.1",
    "react-scripts": "5.0.1",
    "react-toastify": "^10.0.5"
  },
  "devDependencies": {
    "@craco/craco": "^7.1.0",
    "dotenv-cli": "^7.4.2",
    "pm2": "^5.4.2",
    "sass": "^1.77.8"
  },
  "scripts": {
    "start": "dotenv -e ../.env -e .env -- craco start",
    "build": "dotenv -e ../.env -e .env -c production -v BUILD_PATH='dist' -- craco build",
    "serve": "dotenv -e ../.env -e .env -c production -- pm2 serve dist --name @arasnet/portal --spa",
    "stop": "pm2 delete @arasnet/portal"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 2 chrome version",
      "last 2 firefox version",
      "last 2 safari version"
    ]
  },
  "volta": {
    "extends": "../package.json"
  }
}
