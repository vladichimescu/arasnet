{
  "name": "@arasnet/portal",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@ag-grid-community/core": "^32.1.0",
    "@ag-grid-community/infinite-row-model": "^32.1.0",
    "@ag-grid-community/locale": "^32.1.0",
    "@ag-grid-community/react": "^32.1.0",
    "@ag-grid-community/styles": "^32.1.0",
    "@arasnet/functions": "*",
    "@arasnet/types": "*",
    "@fortawesome/fontawesome-free": "^6.6.0",
    "axios": "^1.7.5",
    "date-fns": "^4.1.0",
    "milligram": "^1.4.1",
    "nprogress": "^0.2.0",
    "react": "^18.3.1",
    "react-datepicker": "^7.4.0",
    "react-dom": "^18.3.1",
    "react-i18next": "^15.0.1",
    "react-phone-input-2": "^2.15.1",
    "react-router-dom": "^6.26.1",
    "react-scripts": "5.0.1",
    "react-toastify": "^10.0.5"
  },
  "devDependencies": {
    "@babel/plugin-syntax-import-attributes": "^7.24.7",
    "@craco/craco": "^7.1.0",
    "dotenv-cli": "^7.4.2",
    "esbuild": "^0.23.1",
    "http-server": "^14.1.1",
    "pm2": "^5.4.2",
    "sass": "^1.77.8"
  },
  "scripts": {
    "start": "dotenv -e ../.env -e .env -- craco start",
    "build": "yarn build:react && yarn build:serve",
    "build:react": "dotenv -e ../.env -e .env -c production -v BUILD_PATH='dist' -- craco build && node esbuild.mjs",
    "build:serve": "dotenv -e ../.env -e .env -c production -- node esbuild.mjs",
    "serve": "pm2 start dist/index.mjs --name @arasnet/portal",
    "stop": "pm2 delete @arasnet/portal"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 2 chrome version",
      "last 2 firefox version",
      "last 2 safari version"
    ]
  },
  "volta": {
    "extends": "../package.json"
  }
}
